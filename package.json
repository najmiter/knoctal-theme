{
  "name": "knoctal-theme",
  "displayName": "Knoctal Theme",
  "description": "Knoctal dark theme for VS Code",
  "version": "0.1.6",
  "publisher": "najmiter",
  "engines": {
    "vscode": "^1.60.0"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "icon": "images/icon.png",
  "pricing": "Free",
  "categories": [
    "Themes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/najmiter/knoctal-theme.git"
  },
  "contributes": {
    "themes": [
      {
        "label": "Knoctal Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/knoctal-dark-color-theme.json"
      }
    ],
    "languages": [
      {
        "id": "assembly",
        "aliases": [
          "Assembly",
          "assembly"
        ],
        "extensions": [
          ".asm",
          ".s"
        ]
      },
      {
        "id": "kotlin",
        "aliases": [
          "Kotlin",
          "kotlin"
        ],
        "extensions": [
          ".kt",
          ".kts"
        ]
      }
    ],
    "grammars": [
      {
        "language": "assembly",
        "scopeName": "source.assembly",
        "path": "./syntaxes/assembly.tmLanguage.json"
      },
      {
        "language": "kotlin",
        "scopeName": "source.kotlin",
        "path": "./syntaxes/kotlin.tmLanguage.json"
      }
    ]
  },
  "keywords": [
    "theme",
    "dark",
    "knoctal",
    "assembly theme",
    "assembly syntax",
    "kotlin theme"
  ],
  "license": "CC BY-ND 4.0",
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/vscode": "^1.60.0",
    "typescript": "^5.8.2",
    "vsce": "^2.15.0"
  }
}
